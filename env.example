# Morning Challenge Environment Variables
# 本番環境では必ずこれらの値を変更してください

# === JWT設定 ===
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=30d

# === データベース設定 ===
MONGODB_URI=mongodb://localhost:27017/morning_challenge
MONGODB_MOCK=false

# === サーバー設定 ===
NODE_ENV=development
BACKEND_PORT=5003
FRONTEND_PORT=3004

# === 開発設定 ===
DISABLE_TIME_CHECK=false

# === ログ設定 ===
LOG_LEVEL=info 